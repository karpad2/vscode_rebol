{
    "mysqlsimplecommand": {
		"prefix": "sql",
		"body": [
		"mysql_cmd",
        "[",
        "\"\t$0\"",
        "]"
		],
		"description": "MySQL command"
	},
    "mysqltarcmd": {
		"prefix": "sql tar",
		"body": [
		"mysql_cmd compose",
        "[",
        "\"\t$0\"",
        "]",
        "tar: get_rekordset copy db"
		],
		"description": "MySQL tar command"
	},
    
    "mysqlrekcmd": {
		"prefix": "sql rek",
		"body": [
		"mysql_cmd compose",
        "[",
        "\"\t$0\"",
        "]",
        "rek: get_rekord first db"
		],
		"description": "MySQL rek command"
	},
    "excelcmd": {
		"prefix": "excelcmd",
		"body": [
			"excel_com compose ",
            "[",
            "column \t$0",
			"format \"f:0\" align \"h:Center\" \"v:Center\" \"w:1\" ; header",
			"interior \"c:#E6E6E6\" \"p:Solid\"",
			"\t$4//${0:TODO}: handle exception",
            ";",
            "row  \"h:30\" interior 0 align \"h:Center\" font \"b:1\"",
            "cell (\"Cell1\")",
			"]"
		],
		"description": "excel_com compose"
	},
    "if": {
		"prefix": "if ",
		"body": [
            "if  \t$0",
            "[",
            "\t$1",
            "]"
		],
		"description": "if desc"
	},

    "ifelse": {
		"prefix": "either",
		"body": [
